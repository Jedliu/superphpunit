{
	"name": "superphpunit",
	"displayName": "SuperPhpunit",
	"description": "An extention to run phpunit in laradock environment.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:superphpunit.phpunit"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "superphpunit.phpunit",
				"title": "run laradock phpunit"
			}
		]
	},
	"publisher": "passport4j",
	"repository": {
        "type": "git",
        "url": "https://github.com/Jedliu/superphpunit"
    },
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.52.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	}
}
